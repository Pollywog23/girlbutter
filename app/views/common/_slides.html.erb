

<!-- Page 1 -->
<% @projects.each do |project| %>
<div class="slide-page" id="page-<%= project.id %>">
  <div class="row-fluid">  
    <div class="span6">
      <h2>Details about<span> <%= project.title %></span> and the work I did</h2>
      <p class="lead"><%= project.description %></p>
      <p><a class="btn btn-primary btn-large" href="javascript:$.pageslide.close()">Go Back</a></p>
    </div>  
    <div class="span6">
      <%=image_tag project.image.url(:medium), :alt =>"", :class =>"thumbnail" %>
      <p><em><%= project.title %> </em></p> 
    </div>
  </div>
</div>
<% end %>



